      "source": [
        "def prime(n):\n",
        "  a = 2\n",
        "  c = 0\n",
        "  while c!=n:\n",
        "    flag = 0\n",
        "    for i in range(2,int(a/2+1)):\n",
        "      if(a%i==0):\n",
        "        flag = 1\n",
        "    if flag == 0:\n",
        "      c+=1\n",
        "    a+=1\n",
        "  return a-1"
      ],
